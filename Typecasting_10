#include <stdio.h>
int main()
{
    int a = 5;
    float b = 54 / 5;
    printf("the value of a is %d\n", a);
    printf("the value of b is %f\n", b);
    printf("\n");
    printf("after typecasting\n"); /* typecasting syntax:
                                   (write the type in which want to typecaste) value;*/
    printf("the value of b is %c\n", (char)b);
    printf("the value of b is %d\n", (int)b);
    return 0;


}
